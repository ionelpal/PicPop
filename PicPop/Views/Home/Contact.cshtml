@{
    ViewBag.Title = "Contact Us";
}
<h3 class="text-center text-info">@ViewBag.Title</h3>



<div class="posts">
    <!-- Each entry should be enclosed inside div with the class "entry" -->
    <div class="entry">
        <div class="padd">
            <!-- Contact Page title -->
            <h2>Contact Us</h2>
            <!-- Contact Page Para -->
            <p class="text-success text-center">
                Please fill all the form details and we will get back to you within 48 hours.
            </p>
        </div>

        <div class="row">
            <div class="span5">
                <div class="padd">
                    <div class="co-form">
                        <form class="form-horizontal">
                            <fieldset>
                                <div class="control-group">
                                    <label class="control-label" for="name">Name</label>
                                    <div class="controls">
                                        <input type="text" class="input-medium" id="name">
                                    </div>
                                </div>
                                <div class="control-group">
                                    <label class="control-label" for="email">Email</label>
                                    <div class="controls">
                                        <input type="text" class="input-medium" id="email">
                                    </div>
                                </div>
                                <div class="control-group">
                                    <label class="control-label" for="website">Website</label>
                                    <div class="controls">
                                        <input type="text" class="input-medium" id="website">
                                    </div>
                                </div>
                                <div class="control-group">
                                    <label class="control-label" for="comment">Message</label>
                                    <div class="controls">
                                        <textarea class="input-medium" id="comment" rows="3"></textarea>
                                    </div>
                                </div>
                                <div class="form-actions">
                                    <button type="submit" class="btn">Send</button>
                                </div>
                            </fieldset>
                        </form>
                    </div>
                </div>
            </div>
            <div class="span3">
                <div class="padd">
                    <p class="caddress">
                        <address>
                            <strong>Twitter, Inc.</strong><br>
                            795 Folsom Ave, Suite 600<br>
                            San Francisco, CA 94107<br>
                            <abbr title="Phone">P:</abbr> (001) 456-7890<br><br>
                            <strong>Full Name</strong><br>
                            <a href="mailto:#">admin@picpop.com</a>
                        </address>
                    </p>
                </div>
            </div>
        </div>
        <div class="gmap">
            <div class="padd">
                <!-- Goto Google maps and copy the embed code of your location. Replace the below embed code with your code. -->
                <iframe height="300" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="http://maps.google.co.in/maps?f=q&amp;source=s_q&amp;hl=en&amp;geocode=&amp;q=Google+India+Bangalore,+Bennigana+Halli,+Bangalore,+Karnataka&amp;aq=0&amp;oq=google+&amp;sll=9.930582,78.12303&amp;sspn=0.192085,0.308647&amp;ie=UTF8&amp;hq=Google&amp;hnear=Bennigana+Halli,+Bangalore,+Bengaluru+Urban,+Karnataka&amp;t=m&amp;ll=12.993518,77.660294&amp;spn=0.012545,0.036006&amp;z=15&amp;output=embed"></iframe>
            </div>
        </div>
    </div>
</div>
<!-- Main Content Ends Here -->
        </div>

      </div>

<div class="mobily hidden-desktop">

</div>
    </div>
  </div>
</div>

<!-- Foot -->
<!-- Three column footer -->

<div class="foot">
    <div class="container">
        <div class="row">
            <div class="span4">
                <div class="padd">

                </div>
            </div>


        </div>
    </div>
</div>